{
  "name": "vue3-vite",
  "version": "0.0.0",
  "scripts": {
    "install": "yarn install",
    "reinstall": "rimraf yarn.lock && rimraf package.lock.json && rimraf node_modules && npm run bootstrap",
    "serve": "npm run dev",
    "dev": "vite cross-env NODE_ENV=development --host --open",
    "test": "vite cross-env NODE_ENV=test --host --open",
    "prod": "vite cross-env NODE_ENV=production --host --open",
    "preview": "npm run build && vite preview",
    "preview:dist": "vite preview",
    "build": "vue-tsc --noEmit && vite build",
    "build:no-cache": "yarn clean:cache && npm run build",
    "report": "cross-env REPORT=true npm run build",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "lint:eslint": "eslint \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,ts,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:ls-lint": "ls-lint",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:pretty": "pretty-quick --staged",
    "test:gzip": "http-server dist --cors --gzip -c-1",
    "test:br": "http-server dist --cors --brotli -c-1",
    "install:husky": "is-ci || husky install",
    "gen:icon": "esno ./build/generate/icon/index.ts",
    "postinstall": "npm run install:husky"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.5",
    "@iconify/iconify": "^2.1.0",
    "@vue/runtime-core": "^3.2.26",
    "@vue/shared": "^3.2.26",
    "@vueuse/core": "^7.4.0",
    "@vueuse/shared": "^7.4.0",
    "@zxcvbn-ts/core": "^1.2.0",
    "axios": "^0.24.0",
    "codemirror": "^5.65.0",
    "core-js": "^3.20.0",
    "cropperjs": "^1.5.12",
