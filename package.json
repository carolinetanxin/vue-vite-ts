    "dayjs": "^1.10.7",
    "echarts": "^5.2.2",
    "file-saver": "^2.0.5",
    "intro.js": "^4.3.0",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lowdb": "^3.0.0",
    "nprogress": "^0.2.0",
    "path-to-regexp": "^6.2.0",
    "pinia": "2.0.8",
    "print-js": "^1.6.0",
    "qrcode": "^1.5.0",
    "qs": "^6.10.2",
    "register-service-worker": "^1.7.2",
    "resize-observer-polyfill": "^1.5.1",
    "screenfull": "6.0.0",
    "showdown": "^1.9.1",
    "sortablejs": "^1.14.0",
    "tinymce": "^5.10.2",
    "ua-parser-js": "^1.0.2",
    "vant": "^3.3.7",
    "vditor": "^3.8.9",
    "vue": "^3.2.26",
    "vue-i18n": "^9.1.9",
    "vue-json-pretty": "^2.0.6",
    "vue-router": "^4.0.12",
    "vue-treeselect": "^1.0.7",
    "vue-types": "^4.1.1",
    "vuedraggable": "^2.24.3",
